#include <bits/stdc++.h>
using namespace std;

int main() {
int t,i,j,k,n,m,x;
cin>>t;
while(t--){
    k=0;
    cin>>n>>m;
    vector<int>v;
     vector<int>a2;
     for(i=0;i<n;i++){
         cin>>x;
         v.push_back(x);
     }
     
     for(i=0;i<m;i++){
         cin>>x;
         if(find(v.begin(),v.end(),x)==v.end())
         k=1;
     }
     (k==0)?cout<<"Yes\n":cout<<"No\n";
}

	return 0;
}
